// prisma/schema.prisma

// generator
generator client {
  provider = "prisma-client-js"
}

// This is your Prisma schema file,
datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
}

// models
model Todo {
  id        Int      @id @default(autoincrement())
  input     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}